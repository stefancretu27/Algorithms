CFLAGS = -std=c++17 -Wall -g
CC = g++
ARRAY_GREEDY_SOURCES = min_product_subset.cpp max_product_subset.cpp max_sum_k_negations.cpp max_sum_elementMultipliedWithIndex.cpp maxSum_increasingOrderElementsFromNArrays.cpp maxSum_absoluteDifferencesOfConsecutiveElements.cpp

all:
	$(CC) $(CFLAGS) main.cpp $(ARRAY_GREEDY_SOURCES) -o array_greedy.bin
	
clean:
	rm *.bin
